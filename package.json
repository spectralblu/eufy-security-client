  "repository": {
    "type": "git",
    "url": "git+https://github.com/spectralblu/eufy-security-client.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@cospired/i18n-iso-languages": "^4.1.0",
    "crypto-js": "^4.2.0",
    "date-and-time": "^3.0.3",
